{
    "name": "stev/google-cloud-translate-bundle",
    "description": "Symfony 2 Bundle for Google Cloud Translate Basic API",
    "type": "symfony-bundle",
    "authors": [{
        "name": "Stefan Matei",
        "email": "stev.matei@gmail.com"
    }],
    "keywords": [
        "symfony bundle google cloud translate"
    ],
    "license": [
        "GPL-3.0"
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "ext-json": "*",
        "symfony/framework-bundle": "5.* || 6.*",
        "google/cloud-translate": "^1.12",
        "jms/translation-bundle": "^1.6"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "Stev\\GoogleCloudTranslateBundle\\": ""
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
